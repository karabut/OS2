#include <stdlib.h>
#include <unistd.h>
#include <sys/types.h>
#include <stdio.h>

int main(){

    printf ("Real User ID = %d\n",getuid());
    printf ("Effective User ID = %d\n\n",geteuid());

    FILE * file;

    if (getuid()!=geteuid()){
        printf("No rights to open.\n");
        return 0;
    };

    if((file = fopen("file.txt", "r+")) == NULL) {
        printf ("Cannot open file.\n");
        return 0;
    }





    printf ("File is opened.\n");

    fclose(file);
    return 0;
};

